
public class Overloading {
	
	/// main() start
	public static void main(String[] args) {

	} /// main() end

}

class Person{
	int phoneNum = 0;
	String name;
	
	// 생성자 - 이름
	public Person(String name) {
		this.name = name;
	}
	
	// 생성자 - 이름, 전화번호
	public Person(String name, int phoneNum) {
		this.name = name;
		this.phoneNum = phoneNum;
	}
	
	
}
