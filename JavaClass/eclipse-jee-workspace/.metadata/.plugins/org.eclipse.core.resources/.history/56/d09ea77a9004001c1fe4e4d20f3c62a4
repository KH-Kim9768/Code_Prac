
public class Method {
	
	// --- main() start ---
	public static void main(String[] args) {
		
	}
	
	//
	public static int calcSum(int num1, int num2) {
		int result = num1 + num2;
		return result;
	}
	
	public static double calcSquare(double num1, double num2) {
		double result = num1 * num2;
		return result;
	}
}
