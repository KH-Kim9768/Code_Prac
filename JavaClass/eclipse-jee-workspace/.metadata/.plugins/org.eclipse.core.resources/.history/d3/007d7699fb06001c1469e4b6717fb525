import java.util.List;
import java.util.LinkedList;

public class LinkedList1 {

	public static void main(String[] args) {
		LinkedList<String> linkedList = new LinkedList<>();
		linkedList.add("Lee");
		linkedList.add("Kim");
		linkedList.add("Park");
		
		for (String s : linkedList) {
			System.out.println(s);
		}
		
		linkedList.remove(0);
		
		for (String s : linkedList) {
			System.out.println(s);
		}
		
	}

}
